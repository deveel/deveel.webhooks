<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RootNamespace>Deveel</RootNamespace>
		<VersionPrefix>2.0.1</VersionPrefix>
		<RootNamespace>Deveel</RootNamespace>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Authors>antonello</Authors>
		<Company>Deveel AS</Company>
		<Copyright>Copyright (C) 2021-2023 Deveel AS</Copyright>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageIcon>deveel-logo.png</PackageIcon>
		<RepositoryUrl>https://github.com/deveel/deveel.webhooks</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>webhooks receiver receivers aspnet core aspnetcore httpcontext httprequest twilio sms voice</PackageTags>
		<Description>An implementation of the Deveel Webhooks ASP.NET Core Receiver that is able to receive webhooks from Twilio</Description>
		<PackageProjectUrl>https://deveel.com</PackageProjectUrl>
		<DocumentationFile>.\Deveel.Webhooks.Receiver.Twilio.xml</DocumentationFile>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\..\deveel-logo.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\apl-2.licenseheader" Link="apl-2.licenseheader" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Deveel.Webhooks.Receiver.AspNetCore\Deveel.Webhooks.Receiver.AspNetCore.csproj" />
	</ItemGroup>

</Project>